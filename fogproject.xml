<?xml version="1.0"?>
<Container version="2">
  <Name>fogproject</Name>
  <Repository>mudislander/fogproject</Repository>
  <Registry>https://hub.docker.com/r/mudislander/fogproject//Dockerfile/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Support/>
  <Project>https://fogproject.org/</Project>
  <Overview>A free open-source network computer cloning and management solution</Overview>
  <Category>Network:Other</Category>
  <WebUI>http://[IP]:[PORT:80]/fog</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Mudislander/docker-templates/master/fogproject.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Mudislander/docker-templates/master/img/fogproject_icon.png</Icon>
  <ExtraParams>--security-opt apparmor=mudislander-fogproject</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1553292663</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>A free open-source network computer cloning and management solution</Description>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/FOGserver/</HostDir>
      <ContainerDir>/transfer</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/FOGserver/opt</HostDir>
      <ContainerDir>/opt/fog</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/FOGserver/images</HostDir>
      <ContainerDir>/images</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/TFTPBoot</HostDir>
      <ContainerDir>/tftpboot</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/FOGserver/mysql</HostDir>
      <ContainerDir>mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>80</Value>
      <Name>WEB_HOST_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>192.168.0.200</Value>
      <Name>SERVER_IP</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>/mnt/cache/FOGserver</Value>
      <Name>FOG_DIR</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Veb Host Port" Target="WEB_HOST_PORT" Default="80" Mode="" Description="Container Variable: WEB_HOST_PORT" Type="Variable" Display="always-hide" Required="false" Mask="false">80</Config>
  <Config Name="FOG Directory" Target="/transfer" Default="/mnt/cache/FOGserver/" Mode="rw" Description="Container Path: /transfer" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/FOGserver/</Config>
  <Config Name="opt/" Target="/opt/fog" Default="/mnt/cache/FOGserver/opt" Mode="rw" Description="Container Path: /opt/fog" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/FOGserver/opt</Config>
  <Config Name="Image Directory" Target="/images" Default="" Mode="rw" Description="Container Path: /images" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/FOGserver/images</Config>
  <Config Name="/tftpboot:/tftpboot" Target="/tftpboot" Default="/mnt/cache/TFTPBoot" Mode="rw" Description="Container Path: /tftpboot" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/TFTPBoot</Config>
  <Config Name="Server IP" Target="SERVER_IP" Default="192.168.0.200" Mode="" Description="Container Variable: SERVER_IP" Type="Variable" Display="always" Required="false" Mask="false">192.168.0.200</Config>
  <Config Name="FOG Directory" Target="FOG_DIR" Default="/mnt/cache/FOGserver" Mode="" Description="Container Variable: FOG_DIR" Type="Variable" Display="always" Required="false" Mask="false">/mnt/cache/FOGserver</Config>
  <Config Name="MySQL Directory" Target="mysql" Default="/mnt/cache/FOGserver/mysql" Mode="rw" Description="Container Path: mysql" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/FOGserver/mysql</Config>
</Container>


